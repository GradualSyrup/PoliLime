<%- include("partials/header.ejs") %>

	<div class="container" style="text-align: center;">
		<div class="row">
			<div class="col-1"></div>
			<div class="col-10">
				<h3 id="politicians-title" class="title-adapt-color">
					Politicians
				</h3>
			</div>
			<div class="col-1"></div>
		</div>
		<div class="row">
			<div class="col-2"></div>
			<div class="col-4">
				<h3 class="title-adapt-color">Senate</h3>
				<% Object.keys(members.senate).forEach((state) => { %>
					<ul class="text-adapt-color"><%= state %></ul>
					<% members.senate[state].forEach((member) => { %>
						<li class="text-adapt-color">
							<a href="/search?politicianName=<%= member.split(' ')[0] %>+<%= member.split(' ')[1] %>">
								<%= member %>
							</a>
						</li>
					<% }) %>
				<% }) %>
			</div>
			<div class="col-4">
				<h3 class="title-adapt-color">House</h3>
				<% Object.keys(members.house).forEach((state) => { %>
					<ul class="text-adapt-color"><%= state %></ul>
					<% members.house[state].forEach((member) => { %>
						<li class="text-adapt-color">
							<a href="/search?politicianName=<%= member.split(' ')[0] %>+<%= member.split(' ')[1] %>">
								<%= member %>
							</a>
						</li>
					<% }) %>
				<% }) %>
			</div>
			<div class="col-2"></div>
		</div>
	</div>

<%- include("partials/footer.ejs") %>